{
    walkForward;
    turnLeft;
    walkForward;
    walkForward;
    turnRight;
    {
        walkForward;
    } loop ( n < 5 );
    {
        turnRight;
        {
            walkForward;
        } loop ( n < 2 );
        if ( n == 0)
        {
            print "n = 0";
        }
    } loop ( n < 3);
}
